#!/usr/bin/env jruby
# frozen_string_literal: true

$LOAD_PATH.unshift "./lib"
$LOAD_PATH.unshift File.join(__dir__, "..", "lib")

require "interactive_brokers/generators/api_class_files_generator"

common_code_dir = File.join(__dir__, "..", "lib", "interactive_brokers", "common")
server_code_dir = File.join(__dir__, "..", "app", "java_ext")
InteractiveBrokers::Generators::ApiClassFilesGenerator.new(common_code_dir: common_code_dir,
                                                           server_code_dir: server_code_dir).generate_all
